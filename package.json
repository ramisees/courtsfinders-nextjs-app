{
  "name": "courtsfinders-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 127.0.0.1",
    "dev-all": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "node test-api-fixed.js",
    "test:all": "npm run lint && npm run test:coverage",
    "check-config": "node -e \"require('dotenv').config({path:'.env.local'}); console.log('Google API Key:', process.env.GOOGLE_PLACES_API_KEY ? '✅ Configured' : '❌ Missing')\"",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rimraf .next out"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "framer-motion": "^12.23.3",
    "next": "15.3.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.14",
    "dotenv": "^17.2.0",
    "eslint": "^8",
    "eslint-config-next": "15.3.5",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8",
    "supertest": "^7.1.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
